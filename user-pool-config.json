{
  "PoolName": "KyndlyUserPool",
  "AutoVerifiedAttributes": ["email"],
  "Schema": [
    {
      "Name": "email",
      "Required": true,
      "Mutable": true
    },
    {
      "Name": "tpa_id",
      "AttributeDataType": "String",
      "Mutable": true,
      "Required": false
    },
    {
      "Name": "employer_id",
      "AttributeDataType": "String",
      "Mutable": true,
      "Required": false
    }
  ],
  "MfaConfiguration": "OFF",
  "Policies": {
    "PasswordPolicy": {
      "MinimumLength": 8,
      "RequireUppercase": true,
      "RequireLowercase": true,
      "RequireNumbers": true,
      "RequireSymbols": false
    }
  },
  "AdminCreateUserConfig": {
    "AllowAdminCreateUserOnly": false
  }
} 